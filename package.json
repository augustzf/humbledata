{
  "name": "humbledata",
  "version": "1.0.2",
  "description": "In-memory wrangling of humble-sized data sets",
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "mjs": "tsc -d && mv dist/index.js dist/index.mjs",
    "cjs": "tsc -m commonjs",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "build": "npm run mjs && npm run cjs",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "release": "npm version patch && git push --follow-tags && npm publish"
  },
  "keywords": [
    "big data",
    "small data",
    "data wrangling"
  ],
  "author": "August Flatby",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/augustzf/humbledata"
  },
  "bugs": {
    "url": "https://github.com/augustzf/humbledata/issues"
  },
  "devDependencies": {
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.3",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "prettier": "^1.18.2",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "csv-parse": "^4.6.5"
  }
}
